{
  "compilerOptions": {
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "baseUrl": "src",
      "outDir": "dict",
      "sourceMap": true,
      "noImplicitAny": true,
      "strict": true,
      "typeRoots": ["./src/types", "./node_modules/@types"],
      "types": ["node", "express"],
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}